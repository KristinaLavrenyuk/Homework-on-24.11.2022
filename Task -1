
#include <iostream>
#include <cmath>
#include <iomanip>
using namespace std;

int main()
{
    int x1, y1, x2, y2, a, b, length2;
    double length;
    cin >> x1 >> y1 >> x2 >> y2;

    a = x2 - x1;
    b = y2 - y1;

    length2 = a * a + b * b;
    length = sqrt(length2);

    cout << fixed << setprecision(5) << length << endl;
    system("pause");
}
